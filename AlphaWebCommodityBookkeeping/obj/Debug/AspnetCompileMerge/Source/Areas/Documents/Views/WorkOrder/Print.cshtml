@model AlphaWebCommodityBookkeeping.Areas.Documents.Reports.xrWorkOrder

@{
    ViewBag.Title = "WorkOrderReportPartial";
}


    @Html.DevExpress().ReportViewer(
        settings =>
        {
            settings.Name = "ReportViewerPrint";
            settings.Report = Model;
            settings.CallbackRouteValues = new { Controller = "WorkOrder", Action = "WorkOrderReportPartial", id = ViewData["WorkOrderId"] };
            settings.ExportRouteValues = new { Controller = "WorkOrder", Action = "ReportViewerExportTo", id = ViewData["WorkOrderId"] };
        }
    ).GetHtml()

@model AlphaWebCommodityBookkeeping.Areas.Documents.Reports.xrCashBox

@Html.DevExpress().ReportViewer(
    settings => {
        settings.Name = "ReportViewer1";
        settings.Report = Model;
        settings.CallbackRouteValues = new { Controller = "CashBox", Action = "ReportPartialNormalSize", id = ViewData["CashBoxId"] };
        settings.ExportRouteValues = new { Controller = "CashBox", Action = "ViewerExportToNormalSize", id = ViewData["CashBoxId"] };
    }
).GetHtml()